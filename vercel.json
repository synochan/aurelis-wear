{
  "buildCommand": "cd frontend && npm install && npx vite build --outDir dist",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "env": {
    "VITE_API_URL": "https://aurelis-wear-api.onrender.com/api"
  }
} 