{
  "name": "aurelis-wear-shop",
  "version": "1.0.0",
  "description": "Aurelis Wear Shop - E-commerce application with React frontend and Django backend",
  "private": true,
  "scripts": {
    "frontend": "cd frontend && npm run dev",
